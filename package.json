  "scripts": {
    "start": "webpack-dev-server",
    "start:standalone": "webpack-dev-server --env.standalone",
    "font-init": "npm config set \"@fortawesome:registry\" https://npm.fontawesome.com/ && npm config set \"//npm.fontawesome.com/:_authToken\" 820A812E-C4FC-4313-848E-CC4965165F1E\n",
